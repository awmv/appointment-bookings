      "server": "server/main.ts"
    },
    "testModule": "tests/main.ts"
  },
  "devDependencies": {
    "@types/meteor": "^2.9.7",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/runtime-core": "^3.4.27",
    "@vue/runtime-dom": "^3.4.27",
    "autoprefixer": "^10.4.16",
    "meteor-vite": "^1.7.1",
    "postcss": "^8.4.31",
